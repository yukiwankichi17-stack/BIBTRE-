// 5. Image Memory
const imageMemory: Question[] = [
  {
    id: 5,
    type: 'image-memory',
    title: '短期記憶',
    category: 'Memory',
    prompt: '試験開始時に表示された3枚の画像を、以下の中からすべて選択してください。',
    checkAnswer: (userAnswer, memoryImages) => {
      if (!userAnswer || userAnswer.length !== 3) return false;
      const sortedUserAnswer = [...userAnswer].sort();
      const sortedCorrectAnswer = [...memoryImages].sort();
      return sortedUserAnswer.every((val, index) => val === sortedCorrectAnswer[index]);
    },
  }
];

